<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nengli51.mapper.NewsMapper">
    <select id="findUser" resultType="com.nengli51.domain.News">
        select *
        from news_inf
        where news_id = #{news_id}
    </select>

</mapper>